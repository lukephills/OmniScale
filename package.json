{
  "name": "scales",
  "version": "2.0.5",
  "description": "Synthesizer with musical scales using multi-touch & keyboard",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env.prod",
    "build:android": "npm run build:prepare-cordova && cd-cordova && cordova build android",
    "build:ios": "npm run build:prepare-cordova && cd-cordova && cordova build ios",
    "build:prepare-cordova": "npm-run-all build cordova:clean cordova:copy",
    "cd-cordova": "cd phonegap/scales",
    "clean-and-copy": "npm-run-all clean-dist npm run copy-files",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy app/assets/* dist/assets",
    "cordova:clean": "rimraf phonegap/scales/www",
    "cordova:copy": "cpy dist/* phonegap/scales/www",
    "cordova:icons": "npm run cd-cordova && cordova-icon && cordova-splash",
    "prebuild": "npm run clean-and-copy",
    "start": "webpack-dev-server --env.dev --hot --inline --watch --port 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "zip": "zip -r zipped/bundle.zip dist/*"
  },
  "author": "Luke Phillips",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukephills/OmniScale"
  },
  "bugs": {
    "url": "https://github.com/lukephills/OmniScale/issues"
  },
  "homepage": "http://femurdesign.com/",
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "chai": "3.5.0",
    "copy-webpack-plugin": "^1.1.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "inline-style-prefixer": "^0.6.7",
    "inline-worker": "^1.0.0",
    "install": "^0.8.1",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-mocha": "1.0.1",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "npm-run-all": "1.8.0",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.2",
    "webpack": "2.1.0-beta.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "2.0.0-beta",
    "webpack-hot-middleware": "^2.6.4",
    "webpack-validator": "2.1.2",
    "worker-loader": "^0.7.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "es6-promise": "^3.1.2",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "audio-buffer-utils": "^2.0.1",
    "document-visibility": "^1.0.1",
    "domurl": "^2.1.1",
    "merge-audio-buffers": "^1.0.0",
    "normalize.css": "^3.0.3",
    "worker-timer": "^1.0.0"
  }
}
